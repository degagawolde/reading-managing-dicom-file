   "source": [
    "import glob\n",
    "from pydicom import dcmread\n",
    "filenames = glob.glob('data/*.dcm')\n",
    "p_id = []\n",
    "for file in filenames:\n",
    "    ds = dcmread(file)\n",
    "    p_id.append(ds[0x0010, 0x0020].value)\n",
    "\n",
    "print(\"Unique patient Id {}\".format(len(set(p_id))))"
   ]
  }
